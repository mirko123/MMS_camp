#include<stdio.h>

void squeeze(char s1[], char s2[]) {
    int i ;
    int j ;
    int k;
    
    for( i =0, s1[i] != '\0', i++)
    {
        for(j = 0, s2[j]!='\0' && (s1[i] != s2[j]), j++)
        ;
        if(s2[j] == '\0'){
           s1[k] = s1[i];
           k++;
        }
    }
    
    s1[i] = '\0';
}
